    "argType": "string",
    "typeDescription": "STRING",
    "commandLine": true
},
{
    "type": "value",
    "shortopt": "A",
    "longopt": "artist",
    "description": "Sets artist",
    "group": "Changing tags",
    "argType": "string",
    "typeDescription": "STRING",
    "commandLine": true
},
{
    "type": "value",
    "shortopt": "C",
